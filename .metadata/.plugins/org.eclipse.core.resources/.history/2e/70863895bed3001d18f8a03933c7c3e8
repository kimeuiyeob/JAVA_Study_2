package 람다식;

interface ABA {
	void abc();
}

class BAB {
	static void bcd() {
		System.out.println("메서드메서드");
	}
}

public class RefOfStaticMethod {
	public static void main(String[] args) {

		// 정적 메서드 참조
		// 1 익명 이너클래스 방법
		ABA a = new ABA() {
			@Override
			public void abc() {
				BAB.bcd();
			}
		};
		a.abc();

	}
}
