package 람다식;

interface Z {
	void abc(X x, int k);
}

class X {
	void bcd(int k) {
		System.out.println(k);
	}
}

public class RefOfStaticMethod_Type2_1 {

	public static void main(String[] args) {
		
		//인스턴스 메서드 참조 Type2
		//익명이너 클래스
		
		Z z = new Z() {
			@Override
			public void abc(X x, int k) {
				x.bcd(k);
			}
		};
		z.abc(null, 5);
		
		
		
	}
}
